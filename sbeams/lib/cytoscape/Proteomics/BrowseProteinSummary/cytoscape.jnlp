<?xml version="1.0" encoding="utf-8"?>
<jnlp
  codebase="http://db.systemsbiology.net/tmp/test1"
  href="cytoscape.jnlp">
  <information>
    <title>SBEAMS BrowseProteinSummary to Cytoscape</title>
    <vendor>ISB</vendor>
    <offline-allowed/>
  </information>
  <security>
      <all-permissions/>
  </security>
  <resources>
    <j2se version="1.4+" max-heap-size="1g"/>
    <jar href="CYTOSCAPE_JARS/cytoscape_1.0_merge.jar"/>
    <jar href="data.jar"/>
    <jar href="CYTOSCAPE_JARS/plugins_1.0/getInteractions.jar"/>
    <jar href="CYTOSCAPE_JARS/plugins_1.0/httpdata.jar"/>
    <jar href="CYTOSCAPE_JARS/plugins_1.0/sequence.jar"/>
  </resources>
  <application-desc>
    <argument>-p</argument>
    <argument>jar://project-jnlp</argument>
    <argument>-x</argument>
  </application-desc>
</jnlp>

